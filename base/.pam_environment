NAME		DEFAULT="Kieran Colford"
EMAIL		DEFAULT="kieran\@kcolford.com"

PAGER		DEFAULT=less
LESS		DEFAULT=FRSXi
EDITOR		DEFAULT="emacsclient -a -c"
TEXEDIT		DEFAULT="${EDITOR} +%d %s"
ALTERNATE_EDITOR

PREFIX		DEFAULT=@{HOME}/local
PATH		DEFAULT=${PREFIX}/bin	OVERRIDE=${PREFIX}/bin:${PATH}
CPATH		DEFAULT=${PREFIX}/include OVERRIDE=${PREFIX}/include:${CPATH}
LD_LIBRARY_PATH	DEFAULT=${PREFIX}/lib	OVERRIDE=${PREFIX}/lib:${LD_LIBRARY_PATH}
LIBRARY_PATH	DEFAULT=${PREFIX}/lib	OVERRIDE=${PREFIX}/lib:${LIBRARY_PATH}
XDG_DATA_HOME	DEFAULT=${PREFIX}/var

GOPATH		DEFAULT=${PREFIX}/go	OVERRIDE=${PREFIX}/go:${GOPATH}
GOBIN		DEFAULT=${PREFIX}/bin
NPM_CONFIG_PREFIX DEFAULT=${PREFIX}
PYTHONUSERBASE	DEFAULT=${PREFIX}
