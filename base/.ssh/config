Host uw
	HostName ubuntu1404.student.cs.uwaterloo.ca

Host *.uwaterloo.ca
	User kzlcolfo

Host *
	AddKeysToAgent yes
	CanonicalizeHostname yes
	CanonicalDomains kcolford.com
	ControlMaster auto
	ControlPath ~/.ssh/socket-%C
	ControlPersist 300
	GSSAPIAuthentication yes
	GSSAPIDelegateCredentials yes
	ServerAliveInterval 15
	TCPKeepAlive yes
	VerifyHostKeyDNS yes
